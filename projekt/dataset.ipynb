    "import pandas as pd\n",
    "\n",
    "file_path = \"../new_york.csv.gz\"\n",
    "\n",
    "row_count = 0\n",
    "for chunk in pd.read_csv(file_path, compression='gzip', chunksize=1_000_000):\n",
    "    row_count += len(chunk)\n",
    "\n",
    "print(f\"Total rows: {row_count}\")\n"
   ]
